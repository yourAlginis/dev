using System;
class GFG
{
public class RunLength_Encoding
{
    public static void printRLE(String str)
    {
        int n = str.Length;
        for (int i = 0; i < n; i++)
        {
 
            //  عدد مرات حدوث الحرف الحالي
            int count = 1;
            while (i < n - 1 && str[i] == str[i + 1])
            {
                count++;
                i++;
            }
 
            // هنا هيطلعلك الحروف 
            Console.Write(str[i]);
            Console.Write(count);
        }
    }
 
    public static void Main(String[] args)
    {
        String str = "wwwwaaadexxxxxxywww";
        printRLE(str);
    }
}
}
